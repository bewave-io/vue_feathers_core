{
  "authentication": {
    "authStrategies": [
      "jwt",
      "local"
    ],
    "entity": "user",
    "jwtOptions": {
      "algorithm": "HS256",
      "audience": "https://example.com",
      "expiresIn": "1d",
      "header": {
        "typ": "access"
      }
    },
    "local": {
      "passwordField": "password",
      "usernameField": "email"
    },
    "service": "users"
  },
  "host": "localhost",
  "mongodb": "mongodb://127.0.0.1:27017/backend",
  "origins": [
    "http://localhost:3030"
  ],
  "paginate": {
    "default": 999,
    "max": 999
  },
  "port": 3030,
  "public": "./public/"
}
